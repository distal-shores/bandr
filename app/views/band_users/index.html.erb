<p id="notice"><%= notice %></p>

<h1>Listing Band Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @band_users.each do |band_user| %>
      <tr>
        <td><%= link_to 'Show', band_user %></td>
        <td><%= link_to 'Edit', edit_band_user_path(band_user) %></td>
        <td><%= link_to 'Destroy', band_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Band user', new_band_user_path %>
