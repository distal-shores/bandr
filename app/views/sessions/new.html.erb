

  <div id='login-page'>
  <div class='container'>

    <div>
      <h1>Login</h1>
    </div>

    <%= form_tag sessions_path do %>

    <div class = 'login-form'>
      <%= label_tag :email %><br/>
      <%= text_field_tag :email %>
    </div>
        
    <div class ='login-form' >
      <%= label_tag :password %><br/>
      <%= password_field_tag :password %>
    </div>

    <div id='login-submit'><%= submit_tag "Log In" %>
    </div>
    <% end %>
  </div>
 
</div>
 <img id="concert" src = "/assets/concert.png">

